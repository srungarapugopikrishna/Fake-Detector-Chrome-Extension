{
  "name": "YouTubeFakeDetecter",
  "description": "YouTubeFakeDetecter",
  "version": "1.0",
  "permissions": [
    "activeTab","https://*/*","http://*/*"
  ],
	"content_scripts":[
			{
				"matches": ["http://*.youtube.com/*","https://*.youtube.com/*"],
				"js": ["file.js"]
			}
		],
  "browser_action": {
    "default_icon" : "icon.png",
    "default_title": "YouTubeFakeDetecter"
  },
  
  "manifest_version": 2
}
